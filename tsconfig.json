{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2022",
    "moduleResolution": "node",
    "exactOptionalPropertyTypes": false,
    "useUnknownInCatchVariables": false,
    "noPropertyAccessFromIndexSignature": false
  },
  "include": ["src/**/*"]
}
